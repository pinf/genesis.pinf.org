{
	"boot": {
		"package": "./package.json"
	},
	"config": {
		"genesis.pinf.org/0": {
			"$to": "pgs",
		},
		"github.com/pinf-it/pinf-it-vcs-insight/0": {
			"$to": "pgs-vcs"
		},
		"github.com/pinf-to/pinf-to-sourcemint/0": {
			"$to": "pgs-bundle",
			"catalog": {
				"uid": "{{$from.pgs.uid}}",
				"revision": "{{$from.pgs-vcs.commit}}",
				"name": "{{$from.pgs.basename}}",
				"alias": "{{$from.pgs.basename}}"
			},
			"streams": {
				"master": {
					"id": "github.com/pinf-to/pinf-to-smi/demos/01-HelloWorld",
					"revision": "{{sourceHash()}}",
					"aspects": {
						"archive.tar.gz": {
							"bundler": "tar.gz"
						},
						"archive1.tar.gz": {
							"bundler": "tar.gz"
						}
					}
				}
			}
		},
		"github.com/pinf-to/pinf-to-aws-s3/0": {
			"$to": "pgs-distribution",
			"accessKeyId": "{{env.AWS_ACCESS_KEY}}",
			"secretAccessKey": "{{env.AWS_SECRET_KEY}}",
			"distributions": {
				"pub": {
					"bucket": "{{$from.pgs.basename}}",
					"source": "{{$from.pgs-bundle.path}}"
				}
			}
		},
		"github.com/pinf-it/pinf-it-bundler/0": {
			"target": "browser"
        }
	}
}