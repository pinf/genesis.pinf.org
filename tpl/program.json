{
	"name": "boot",
	"boot": {
		"package": "./package.json"
	},
	"config": {
		"boot/0": {
			"$to": "pgs-boot",
			"revision": "{{deriveRevision()}}",
			"systemRoot": "{{env.PGS_SYSTEM_ROOT}}",
			"variables": {
				"UID": "{{ensureUid()}}",
				"EXTENDS": "[\"{{__DIRNAME__}}/*.proto.json\"]",
				"BASENAME": "{{deriveBasename()}}"
			}
		}
	}
}