{
	"uid": "expand.genesis.pinf.org",
	"boot": {
		"config": "expand.genesis.pinf.org/0",
		"package": "./package.json"
	},
	"config": {
		"expand.genesis.pinf.org/0": {
			"$to": "pgs-boot",
			"revision": "{{$from.pgs-boot-filetree.hash}}",
			"workspaceRoot": "{{env.PGS_WORKSPACE_ROOT}}",
			"directories": {
				"pinf": "{{env.PGS_PINF_DIR}}",
				"packages": "{{env.PGS_PACKAGES_DIR}}"
			},
			"workspaceVariables": {
				"UID": "{{ensureUid()}}",
				"BASENAME": "{{deriveBasename()}}",
				"EXTENDS": "[\"{{env.PGS_WORKSPACE_ROOT}}/{{env.PGS_PACKAGES_DIR}}/github.com~devcomp~devcomp.org~0~source/*.proto.json\"]",
				"OVERLAYS": "[\"!{{env.PGS_WORKSPACE_ROOT}}/PINF.json\"]",
				"PGSPROG": "{{__DIRNAME__}}"
			},
			"workspaceFiles": "{{getWorkspaceFiles()}}",
			"workspaceFilesVariables": {
				".gitignore": {
					"%%CUSTOMRULES%%": ""
				},
				".distignore": {
					"%%CUSTOMRULES%%": ""
				}
			},
			"@translocate": "!{{env.PGS_WORKSPACE_ROOT}}/PINF.json"
		},
		"github.com/pinf-it/pinf-it-filetree-insight/0": {
			"$to": "pgs-boot-filetree",
			"watch": true
		}
	}
}